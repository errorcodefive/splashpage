{
  "name": "splashpage",
  "version": "1.0.0",
  "description": "splashpage",
  "main": "server.js",
  "scripts": {
    "watch_test": "nyc --reporter=lcov mocha test/test-server.js -w --require babel-register --exit",
    "test": "mocha --timeout 10000 --exit",
    "coverage": "nyc --reporter=lcov --reporter=text mocha --timeout 10000 --exit",
    "compile": "babel src --presets @babel/react,@babel/preset-env --out-dir client",
    "watch": "babel src --presets @babel/react,@babel/preset-env --out-dir client --watch",
    "start": "SET NODE_ENV=development&& nodemon -w server.js server.js",
    "webpack_build": "webpack --mode production",
    "webpack_start": "webpack-dev-server --port 8080 -d --open --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errorcodefive/splashpage.git"
  },
  "author": "Kyle Chin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/errorcodefive/splashpage/issues"
  },
  "homepage": "https://github.com/errorcodefive/splashpage#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "body-parser": "^1.18.3",
    "config": "^1.31.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.7",
    "nodemon": "^1.18.5",
    "npm": "^6.4.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-modal": "^3.6.1",
    "react-notifications": "^1.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.4",
    "nyc": "^12.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
